<Page x:Class="Andreeva_TZv2.SUser.SUserLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Andreeva_TZv2.SUser"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SUserLogin">

    <Grid>
        <Button x:Name="loginSuperUser_btn" Content="Reg" Width="200" Height="40" Margin="0,290,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Click="loginSuperUser_btn_Click"/>
        <TextBox x:Name="loginSuperUser_tb" Style="{DynamicResource TextBoxK}" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" FontSize="32" Height="40" Width="150"/>
        <PasswordBox x:Name="passwordSuperUser_tb" HorizontalAlignment="Center" Margin="0,190,0,0"  VerticalAlignment="Top" FontSize="32" Height="40" Width="150"/>

    </Grid>
</Page>
